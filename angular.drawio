//angular.io/guide/built-in-directives#ngClass&quot;&gt;&lt;span class=&quot;s1&quot;&gt;ngClass&lt;/span&gt;&lt;/a&gt;&lt;span&gt;).&lt;/span&gt;&lt;br&gt;&lt;/p&gt;" style="text;whiteSpace=wrap;html=1;fontSize=17;" vertex="1" parent="jCv_f7iNkOVhFn-Z_VQX-1">
